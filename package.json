{
    "name": "@danwithabox/eslint-entree",
    "version": "0.3.2",
    "description": "An appetizer for linting, focusing on incremental, non-jarring adoption of rules, with the provided ESLint flat config utilities",
    "type": "module",
    "engines": {
        "node": "^18.18.0 || >=20.0.0"
    },
    "keywords": [
        "eslint-config",
        "eslint-entree",
        "eslint",
        "starter",
        "adoption",
        "typescript",
        "vue",
        "vue3"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./lib/index.d.cts",
                "default": "./lib/index.cjs"
            },
            "import": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.js"
            }
        }
    },
    "files": [
        "lib"
    ],
    "author": "danwithabox <danwithabox@gmail.com> (https://github.com/danwithabox)",
    "license": "MIT",
    "scripts": {
        "build": "tsup && publint",
        "prepack": "npm run build",
        "prepublishOnly": "npm run build",
        "type-check": "tsc --noEmit"
    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "publint": "^0.2.8",
        "tsup": "^8.1.0",
        "typescript": "^5.5.3",
        "vue": "^3.4.31"
    },
    "peerDependencies": {
        "eslint": "^9.7.0"
    },
    "dependencies": {
        "@stylistic/eslint-plugin": "^2.3.0",
        "eslint-config-flat-gitignore": "^0.1.7",
        "eslint-define-config": "^2.1.0",
        "eslint-plugin-vue": "^9.27.0",
        "type-fest": "^4.21.0",
        "typescript-eslint": "^8.0.0-alpha.41",
        "vue-eslint-parser": "^9.4.3"
    }
}
