{
    "name": "@danwithabox/eslint-entree",
    "version": "0.6.1",
    "description": "An appetizer for linting ðŸ¥‚",
    "type": "module",
    "engines": {
        "node": "^18.18.0 || >=20.0.0",
        "pnpm": ">=8.0.0"
    },
    "volta": {
        "node": "20.18.1",
        "pnpm": "9.14.4"
    },
    "packageManager": "pnpm@9.14.4",
    "keywords": [
        "eslint",
        "eslint-entree",
        "entree",
        "appetizer",
        "starter",
        "adoption",
        "config",
