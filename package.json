{
    "name": "@danwithabox/eslint-entree",
    "version": "0.1.8",
    "description": "ESLint flat config utilities to make config prototyping and usage easier",
    "type": "module",
    "engines": {
        "node": "^18.18.0 || >=20.0.0"
    },
    "keywords": [
        "eslint-config"
    ],
    "main": "./lib/index.cjs",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./lib/index.d.cts",
                "default": "./lib/index.cjs"
            },
            "import": {
                "types": "./lib/index.d.ts",
                "default": "./lib/index.js"
            }
        }
    },
    "files": [
        "lib"
    ],
    "author": "danwithabox <danwithabox@gmail.com> (https://github.com/danwithabox)",
    "license": "MIT",
    "scripts": {
        "build": "tsup && publint",
        "prepack": "npm run build",
        "prepublishOnly": "npm run build",
        "type-check": "tsc --noEmit"
    },
    "devDependencies": {
        "@types/node": "^20.11.19",
        "publint": "^0.2.7",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "eslint": "^8.56.0"
    },
    "dependencies": {
        "eslint-define-config": "^2.1.0",
        "type-fest": "^4.10.2"
    }
}
